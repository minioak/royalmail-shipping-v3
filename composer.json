{
    "name": "mobi-market/royalmail-shipping-v3",
    "type": "library",
    "description": "Royal Mail Shipping API V3 Laravel package",
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^6.3|^7.3",
        "illuminate/support": ">=5.8 <10.0",
        "league/oauth2-client": "^2.7"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Callum Slaney-Flett",
            "email": "callum@mobi-market.co.uk"
        },
        {
            "name": "Scott-David Jones",
            "email": "scott.jones@autumndev.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "MobiMarket\\RoyalMailShipping\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MobiMarket\\RoyalMailShipping\\RoyalMailServiceProvider"
            ],
            "aliases": {
                "RoyalMail": "MobiMarket\\RoyalMailShipping\\RoyalMailFacade"
            }
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
